{
  "name": "lookfeel/laravel-eloquent-append-automate",
  "description": "根据 appends 配置的关联字段映射关系，自动维护需要的 appends。",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel appends",
    "laravel accessors"
  ],
  "authors": [
    {
      "name": "TerranC",
      "email": "terran.chao@lookfeel.co"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/database": "~5.4",
    "illuminate/console": "~5.4",
    "illuminate/support": "~5.4",
    "illuminate/cache": "~5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "laravel/laravel": "~5.4",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Lookfeel\\AppendAutomate\\": "src"
    },
    "files": [
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lookfeel\\AppendAutomate\\AppendAutomateServiceProvider"
      ]
    }
  }
}
